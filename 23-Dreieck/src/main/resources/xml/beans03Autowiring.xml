<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


	<bean id="dreieck" class="edu.spring.dreieck.xml.autowiring.Dreieck" autowire="byName" />
	
	<bean id="punktA" class="edu.spring.dreieck.xml.autowiring.Punkt">
		<property name="x" value="1"></property>
		<property name="y" value="1"></property>
	</bean>
	
	<bean id="punktB" class="edu.spring.dreieck.xml.autowiring.Punkt">
		<property name="x" value="2" />
		<property name="y" value="2" />
	</bean>
	
	<bean id="punktC" class="edu.spring.dreieck.xml.autowiring.Punkt">
		<property name="x" value="3" />
		<property name="y" value="3" />
	</bean>

</beans>
